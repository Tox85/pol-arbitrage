{
  "name": "poly5",
  "version": "1.0.0",
  "description": "Bot de Market Making pour Polymarket",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node --watch src/index.ts",
    "check-balances": "ts-node scripts/check-balances.ts",
    "find-proxy": "ts-node scripts/find-proxy.ts",
    "test-poly-sdk": "ts-node scripts/test-poly-sdk.ts",
    "test-websocket": "ts-node scripts/test-websocket.ts",
    "smoke": "node scripts/smoke.js",
    "audit:dup": "jscpd --min-tokens 70 --threshold 1 --reporters json --output .audit --pattern \"src/**/*.ts\" || true",
    "audit:cov": "c8 --reporter=json node scripts/smoke.js",
    "audit:all": "mkdir -p .audit && npm run build && npm run audit:dup && npm run audit:cov"
  },
  "keywords": [
    "polymarket",
    "market-making",
    "trading",
    "bot"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ethersproject/wallet": "^5.8.0",
    "@polymarket/clob-client": "^4.22.3",
    "@polymarket/order-utils": "^2.1.0",
    "axios": "^1.12.2",
    "dotenv": "^17.2.3",
    "ethers": "^6.15.0",
    "pino": "^9.12.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/node": "^24.6.1",
    "@types/ws": "^8.18.1",
    "c8": "^10.1.3",
    "eslint-plugin-import": "^2.32.0",
    "jscpd": "^4.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  }
}
